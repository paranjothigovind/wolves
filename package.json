  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "fs": "0.0.1-security",
    "googleapis": "^63.0.0",
    "http-proxy": "^1.18.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.3.0",
    "lokijs": "^1.5.11",
    "mongodb": "^3.6.2",
    "mongoose": "^5.3.11",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.14",
    "nodemon": "^2.0.6",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "razorpay": "^2.0.6",
    "react-multiselect-checkboxes": "^0.1.1",
    "readline": "^1.3.0",
    "shortid": "^2.2.16",
    "stripe": "^8.119.0",
    "twilio": "^3.50.0",
    "validator": "^10.9.0"
  }
